//this is my fav code snippet as it prints the array in waveform
#include<iostream>
using namespace std;
int main()
{
 int arr[10][10],m,n,i,j;
 cin>>m>>n;
 for(i=0;i<n;i++)
 {
  for(j=0;j<n;j++)
  cin>>arr[i][j];
 }
 for(j=0;j<n;j++)
 {
  if((j%2)==0)
  for(i=0;i<m;i++)
  cout<<arr[i][j]<<",";
  else
  for(i=m-1;i>=0;i--)
  cout<<arr[i][j]<<",";
}
 cout<<"END";
 return 0;
}
